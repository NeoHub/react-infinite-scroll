{
  "name": "@srph/react-infinite-scroll",
  "version": "0.1.0",
  "description": "A simple infinite scroll React.js component",
  "main": "lib/infinite.js",
  "scripts": {
    "example": "webpack -v --progress --config=docs/webpack.config.js",
    "example:run": "npm run example && open docs/index.html",
    "example:start": "webpack --config=docs/webpack.config.js -wv --progress && open docs/index.html",
    "prepublish": "babel index.js -o dist/infinite.js"
  },
  "keywords": [
    "react",
    "infinite",
    "scroll"
  ],
  "repository": "https://github.com/srph/react-infinite-scroll",
  "author": "Kier Borromeo <seraphipod@gmail.com> (http://srph.github.io)",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "react": "^15.3.2",
    "dom-helpers": "^2.4.0",
    "react-dom": "^15.3.2"
  }
}
